{
  "name": "Style Cleanser",
  "description": "Make the internet More Readable",
  "version": "1",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "background": {
    "scripts": [ "jquery-3.1.1.min.js", "content.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Make this Text more Readable",
    "default_icon": "SCicon.png"
  },
  "manifest_version": 2
}
